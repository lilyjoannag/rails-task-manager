<h1>New Task</h1>
<div class="new-task-form">
  <%= form_for(@task) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :details %>
    <%= f.text_field :details %>
    <%= f.submit class: 'btn btn-primary'%>
  <% end %>
</div>
<%= link_to "Back to list", tasks_path %>
